<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="org.iuts3a.domain.User" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.Authority" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.User.authorities" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.PersistentToken" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="org.iuts3a.social.SocialUserConnection" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.Address" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.Customer" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.Customer.orders" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.Offer" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.SalesQuote" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.SalesQuoteItem" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.SalesOrder" uses-template="simple"/>

    <cache alias="org.iuts3a.domain.SalesOrderItem" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
